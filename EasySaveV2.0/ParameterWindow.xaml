<Window x:Class="EasySaveV2._0.ParameterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasySaveV2._0"
        mc:Ignorable="d"
        Title="ParameterWindow" Height="359" Width="229" ResizeMode="NoResize">
    <Grid>
        <Label Content="{Binding LanguageLabel}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="71"/>
        <ComboBox Margin="100,13,19,276" SelectedItem="{Binding LanguageBoxCommand}" >
            <ComboBoxItem>Français</ComboBoxItem>
            <ComboBoxItem>English</ComboBoxItem>
        </ComboBox>
        <Label Content="{Binding ExtensionLogLabel}" HorizontalAlignment="Left" Margin="10,57,0,0" VerticalAlignment="Top" Width="90"/>
        <ComboBox Margin="105,60,14,226" SelectedItem="{Binding ExtensionBoxCommand}">
            <ComboBoxItem>Json</ComboBoxItem>
            <ComboBoxItem>Xml</ComboBoxItem>
        </ComboBox>
        <Label Content="{Binding WhiteListeLabel}" HorizontalAlignment="Left" Margin="10,111,0,0" VerticalAlignment="Top" Width="176" Height="30"/>
        <TextBox Text="{Binding WhiteListCryptage}" HorizontalAlignment="Left" Height="94" Margin="11,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="176"/>
        <Label Content="{Binding ProcessusMetierLabel}" HorizontalAlignment="Left" Margin="11,254,0,0" VerticalAlignment="Top" Width="176"/>
        <TextBox HorizontalAlignment="Left" Margin="11,285,0,0" Text="{Binding ProcessusWork}" TextWrapping="Wrap" VerticalAlignment="Top" Width="144"/>
    </Grid>
</Window>
